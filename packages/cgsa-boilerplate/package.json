{
  "name": "boilerplate",
  "version": "0.0.1",
  "description": "GlueStack Boilerplate",
  "scripts": {
    "install-all": "npm install --workspces --if-present",
    "build:components": "npm run build --workspace @project/components",
    "watch:components": "npm run watch --workspace @project/components",
    "start:mobile": "npm run start --workspace @project/mobile",
    "start:storybook": "npm run storybook --workspace @project/storybook",
    "start:website": "npm run dev --workspace @project/website"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gluestack/create-gluestack-app",
    "directory": "cgsa-boileplate"
  },
  "keywords": [
    "npm",
    "create-gluestack-app"
  ],
  "engines": {
    "node": ">=18 <19"
  },
  "license": "ISC",
  "dependencies": {
    "@dank-style/react": "^0.3.8",
    "@gluestack-ui/button": "^0.1.4",
    "@gluestack-ui/provider": "^0.1.1",
    "@gluestack/framework": "^0.0.x",
    "@gluestack/glue-plugin-backend-engine": "^0.0.x",
    "@gluestack/glue-plugin-engine": "^0.0.95",
    "@gluestack/glue-plugin-mobile-expo": "^0.0.5",
    "@gluestack/glue-plugin-router-nginx": "^0.0.x",
    "@gluestack/glue-plugin-storybook": "^1.1.x",
    "@gluestack/glue-plugin-web": "^0.1.32",
    "@gluestack/ui-next-adapter": "^2.0.2",
    "@legendapp/motion": "^2.2.0"
  },
  "peerDependencies": {
    "@gluestack/framework": "^0.0.x"
  },
  "workspaces": [
    "storybook",
    "shared/components",
    "backend/engine",
    "mobile",
    "website"
  ]
}
